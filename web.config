<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.webServer>
        <staticContent>
	        <httpProtocol>
                <customHeaders>
                  <remove name="X-Frame-Options" />
                    <add name="X-Frame-Options" value="SAMEORIGIN" />
                </customHeaders>
            </httpProtocol>
        </staticContent>
    </system.webServer>
</configuration>
